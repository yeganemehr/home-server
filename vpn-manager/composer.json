{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "yeganemehr/vpn-manager",
    "bin": ["vpn-manager"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel-zero/framework": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
